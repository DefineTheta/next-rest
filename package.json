{
	"name": "next-rest",
	"version": "0.4.3",
	"description": "Typesafe REST APIs for Next.js",
	"files": [
		"api/*.d.ts",
		"api/*.js",
		"server/*.js",
		"server/*.d.ts",
		"client/*.js",
		"client/*.d.ts"
	],
	"exports": {
		"./api": {
			"types": "./api/index.d.ts",
			"default": "./api/index.js"
		},
		"./client": {
			"types": "./client/index.d.ts",
			"default": "./client/index.js"
		},
		"./server": {
			"types": "./server/index.d.ts",
			"default": "./server/index.js"
		}
	},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"build": "tsc --build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/joeltg/next-rest.git"
	},
	"author": "Joel Gustafson",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/joeltg/next-rest/issues"
	},
	"homepage": "https://github.com/joeltg/next-rest#readme",
	"devDependencies": {
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"typescript": "^4.4.4"
	},
	"peerDependencies": {
		"next": "^12.0.2",
		"react": "^17.0.2 || ^18.0.0",
		"react-dom": "^17.0.2 || ^18.0.0"
	},
	"dependencies": {
		"@types/node": "^16.11.6",
		"@types/react": "^17.0.34",
		"@types/react-dom": "^17.0.11",
		"http-status-codes": "^2.1.4"
	}
}
